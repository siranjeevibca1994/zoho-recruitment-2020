import java.util.*; 
class SnakeMatrix 
{ 
 static void print(int mat) 
	    { 
        	String strval = "";
        	int count = 0 ;
	        for (int i = 1; i <= mat; i++) 
	        { 
	        	int val = mat * i;
				for (int j = 0; j < val; j++){
					count = strval.length();
		        	strval += count <=j ? String.valueOf(j+1) : "";

				} 
	        } 
	        System.out.println(strval);
	    } 
	  
	    public static void main(String[] args) 
	    {  
	        print(4); 
	    }  
} 
